pipeline {
    agent any
    stages {
        stage('Run The Example 1') {
            steps {
                grep -o -i "devops" example.txt | wc -l
            }
        }
        stage('Run The Example 2') {
            steps {
                sed -i.bak 's/devops/hello_world/g' file1.txt
            }
        }
    }
}
